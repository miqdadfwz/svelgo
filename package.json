{
  "name": "svelte-vite-ssr",
  "version": "0.0.1",
  "description": "svelte boilerplate with ssr and type support",
  "main": "index.js",
  "scripts": {
    "dev:client": "NODE_ENV=development vite --port 3001",
    "dev:server": "NODE_ENV=development SSR_MODE=1 PORT=3000 node server.dev.js",
    "lint": "eslint '*/**/*.{js,ts,tsx,svelte}' --quiet --fix",
    "husky:prepare": "husky install",
    "pre-commit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/miqdadfwz/svelte-vite-ssr.git"
  },
  "keywords": [
    "svelte",
    "ssr",
    "vite",
    "typescript"
  ],
  "author": "Miqdad Abdurrahman Fawwaz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/miqdadfwz/svelte-vite-ssr/issues"
  },
  "homepage": "https://github.com/miqdadfwz/svelte-vite-ssr#readme",
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@fullhuman/postcss-purgecss": "^4.0.3",
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.12",
    "@tsconfig/svelte": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "autoprefixer": "^10.3.1",
    "cssnano": "^5.0.6",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv-safe": "^8.2.0",
    "eslint": "^7.30.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-svelte3": "^3.2.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.0.1",
    "postcss": "^8.2.8",
    "postcss-load-config": "^3.1.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.3.2",
    "prettier-plugin-svelte": "^2.3.1",
    "svelte-preprocess": "^4.7.4",
    "tailwindcss": "^2.2.4",
    "typescript": "^4.3.5",
    "vite": "^2.4.2",
    "vite-tsconfig-paths": "^3.3.13"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@koa/router": "^10.0.0",
    "koa": "^2.13.1",
    "koa-compress": "^5.0.1",
    "koa-connect": "^2.1.0",
    "koa-pino-logger": "^3.0.0",
    "pino-colada": "^2.1.0",
    "svelte": "^3.38.3"
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "maintained node versions"
  ]
}
